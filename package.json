{
  "name": "ulbi-production",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "eslint": "eslint \"**/*.{ts,tsx}\"",
    "eslint:fix": "eslint \"**/*.{ts,tsx}\" --fix\n",
    "stylelint": "stylelint **/*.scss",
    "stylelint:fix": "stylelint **/*.scss --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "yarn loki test",
    "test:ui:approve": "yarn loki approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "reg:json": "node scripts/generate-visual-json-report.js",
    "reg:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook"
  },
  "author": "Dima <ctaricc@mail.ru>",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-redux": "^7.1.25",
    "i18next": "21.6.11",
    "i18next-browser-languagedetector": "6.1.3",
    "i18next-http-backend": "1.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.15.5",
    "react-redux": "^8.0.5",
    "react-router-dom": "6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-essentials": "7.0.17",
    "@storybook/addon-interactions": "7.0.17",
    "@storybook/addon-links": "7.0.17",
    "@storybook/blocks": "7.0.17",
    "@storybook/react": "7.0.17",
    "@storybook/react-webpack5": "7.0.17",
    "@storybook/testing-library": "0.0.14-next.2",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/jest": "^29.5.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-dev-server": "4.7.2",
    "@typescript-eslint/eslint-plugin": ">=5.57.0",
    "@typescript-eslint/parser": ">=5.57.0",
    "babel-loader": "8.2.3",
    "babel-plugin-i18next-extract": "^0.9.0",
    "css-loader": "6.6.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-i18next": "^6.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "loki": "^0.31.2",
    "mini-css-extract-plugin": "2.5.3",
    "react-refresh": "^0.14.0",
    "reg-cli": "^0.17.7",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.62.1",
    "sass-loader": "12.6.0",
    "storybook": "7.0.17",
    "style-loader": "3.3.1",
    "stylelint": "^15.6.2",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "ts-loader": "9.2.6",
    "ts-node": "10.5.0",
    "typescript": ">=4.4",
    "webpack": "5.69.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "4.7.4"
  },
  "loki": {
    "looks-same": {
      "ignoreCaret": true
    },
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
